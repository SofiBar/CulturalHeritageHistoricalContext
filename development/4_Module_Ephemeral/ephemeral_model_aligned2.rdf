        <rdfs:label xml:lang="el">P4 βρισκόταν σε εξέλιξη</rdfs:label>
        <rdfs:label xml:lang="ru">P4 имеет временной отрезок</rdfs:label>
        <rdfs:label xml:lang="zh">P4 发生时段是</rdfs:label>
