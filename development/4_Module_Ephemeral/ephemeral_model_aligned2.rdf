        <skos:example># June 2004 to August 2006

:when a time:ProperInterval ;
  edtfo:hasEDTFDateTimeDescription &quot;2004-06/2006-08&quot;^^edtf:EDTF
  .</skos:example>
    </rdf:Description>
    


